%{
/*
* this sample demonstrates (very) simple recogntion:
* a verb or not a verb
*/
%}
%%

[\t ]+		/* ignorewhitespace */ ;


did |
have |
go			{ printf("%s: is a verb\n", yytext); }

[a-zA-Z]+	{ printf("%s: is not a verb\n", yytext); }

.|\n 		{ ECHO; /* normal default anyway */ }
%%

int main()
{
	yylex();
	return 0;
}